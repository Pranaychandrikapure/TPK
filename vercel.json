{
    "builds": [
        {
            "src": "bookmyseat/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "Python 3.11.0"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bookmyseat/wsgi.py"
        }
    ]
}